{
  "name": "genshin-search-bot",
  "version": "1.0.0",
  "description": "디스코드 원신 서치봇",
  "main": "./dist/worker.js",
  "scripts": {
    "update-package": "ncu -u && npm i",
    "dev": "wrangler dev --var DISCORD_PUBLIC_KEY:$DISCORD_PUBLIC_KEY --persist-to ./kv/",
    "deploy": "wrangler publish && node ./src/upload.js",
    "build": "node ./esbuild.config.js",
    "prebuild": "node ./src/prebuild.js",
    "upload": "node ./src/upload.js"
  },
  "keywords": [
    "genshin",
    "discord",
    "search",
    "bot"
  ],
  "author": "Ariette",
  "license": "MIT",
  "dependencies": {
    "cookie": "^0.6.0",
    "discord-api-types": "^0.37.70",
    "discord-interactions": "^3.4.0",
    "hoyoapi": "^1.2.0",
    "table": "^6.8.1"
  },
  "devDependencies": {
    "@types/cookie": "^0.6.0",
    "esbuild": "^0.20.1",
    "esbuild-node-externals": "^1.13.0",
    "npm-check-updates": "^16.14.15",
    "shelljs": "^0.8.5",
    "typescript": "^5.3.3",
    "wrangler": "^3.28.3"
  }
}
