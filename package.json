{
  "name": "genshin-search-bot",
  "version": "1.0.0",
  "description": "디스코드 원신 서치봇",
  "main": "./dist/worker.js",
  "scripts": {
    "update-package": "ncu -u && npm i",
    "start": "NODE_ENV=development miniflare ./dist/worker.js --watch --debug --binding DISCORD_PUBLIC_KEY=$DISCORD_PUBLIC_KEY",
    "deploy": "wrangler publish && node ./src/upload.js",
    "build": "node ./esbuild.config.js",
    "prebuild": "node ./src/prebuild.js",
    "upload": "node ./src/upload.js"
  },
  "keywords": [
    "genshin",
    "discord",
    "search",
    "bot"
  ],
  "author": "Ariette",
  "license": "MIT",
  "dependencies": {
    "cookie": "^0.5.0",
    "discord-api-types": "^0.37.35",
    "discord-interactions": "^3.3.0",
    "genshin-db": "^4.3.6",
    "table": "^6.8.1"
  },
  "devDependencies": {
    "@types/cookie": "^0.5.1",
    "esbuild": "^0.17.10",
    "esbuild-node-externals": "^1.6.0",
    "miniflare": "^2.12.1",
    "npm-check-updates": "^16.7.10",
    "shelljs": "^0.8.5",
    "typescript": "^4.9.5",
    "wrangler": "^2.12.0"
  }
}
